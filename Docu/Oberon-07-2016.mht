        PROCEDURE WriteInt*(x, n: INTEGER);
                VAR i: INTEGER; a: ARRAY 16 OF CHAR;
        BEGIN i :=3D 0;
                IF x &lt; 0 THEN Texts.Write(W, "-"); x :=3D -x END ;
