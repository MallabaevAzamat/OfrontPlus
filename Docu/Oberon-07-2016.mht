<pre>PROCEDURE ReadInt(VAR x: INTEGER);
    VAR i : INTEGER; ch: CHAR;
BEGIN i :=3D 0; Read(ch);
    WHILE ("0" &lt;=3D ch) &amp; (ch &lt;=3D "9") DO
            i :=3D 10*i + (ORD(ch)-ORD("0")); Read(ch)
    END ;
    x :=3D i
END ReadInt

PROCEDURE WriteInt(x: INTEGER); (* 0 &lt;=3D x &lt; 10^5 *)
    VAR i: INTEGER;
            buf: ARRAY 5 OF INTEGER;
BEGIN i :=3D 0;
    REPEAT
       buf[i] :=3D x MOD 10; x :=3D x DIV 10; INC(i)
    UNTIL x =3D 0;
    REPEAT DEC(i); Write(CHR(buf[i] + ORD("0"))) UNTIL i =3D 0
END WriteInt

PROCEDURE log2(x: INTEGER): INTEGER;
    VAR y: INTEGER; (*assume x&gt;0*)
BEGIN y :=3D 0;
    HILE x &gt; 1 DO x :=3D x DIV 2; INC(y) END ;
    RETURN y
END log2
</pre>


