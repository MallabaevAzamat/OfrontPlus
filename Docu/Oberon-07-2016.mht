                REPEAT Texts.Write(W, " "); DEC(n) UNTIL n &lt;=3D i;
                REPEAT DEC(i); Texts.Write(W, a[i]) UNTIL i =3D 0
        END WriteInt;

        PROCEDURE WriteLn*;
        BEGIN Texts.WriteLn(W); Texts.Append(Oberon.Log, W.buf)
        END WriteLn;

BEGIN Texts.OpenWriter(W)
END Out.
